{
  "name": "poker-ranges",
  "main": "build/electron.js",
  "homepage": "./",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/runtime": "^7.7.5",
    "@material-ui/core": "^4.7.2",
    "@material-ui/icons": "^4.5.1",
    "@reduxjs/toolkit": "^1.1.0",
    "bootstrap": "^4.3.1",
    "electron-is-dev": "^1.1.0",
    "graceful-fs": "^4.2.3",
    "lodash": "^4.17.15",
    "material-ui-slider": "^3.0.8",
    "npm": "^6.13.2",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.14",
    "react-dom": "^16.11.0",
    "react-redux": "^7.1.1",
    "react-resizable": "^1.10.1",
    "react-scripts": "3.3.0",
    "redux": "^4.0.4",
    "styled-components": "^4.0.0",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "package": "electron-builder build --win -c.extraMetadata.main=build/electron.js --publish never",
    "test": "xo && react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none REACT_APP_CWD=\"$(pwd)\" yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "plugins": [
      "unicorn"
    ],
    "rules": {
      "unicorn/filename-case": [
        "error",
        {
          "case": "pascalCase"
        }
      ],
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "comma-dangle": 0
    },
    "globals": {
      "document": true,
      "window": true
    },
    "parser": "babel-eslint"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "portable"
    },
    "npmRebuild": false,
    "compression": "store"
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.2.0",
    "electron-packager": "^14.1.1",
    "eslint": "^6.7.2",
    "eslint-config-xo": "^0.27.2",
    "eslint-config-xo-react": "^0.20.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "use-why-did-you-update": "^0.1.0",
    "wait-on": "^3.3.0",
    "xo": "^0.25.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
